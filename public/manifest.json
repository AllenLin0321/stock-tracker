{
  "manifest_version": 2,
  "name": "Stock Tracker",
  "description": "This extension is a Stock Tracker APP",
  "version": "0.0.1",
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "favicon.png",
    "default_title": "Open the popup"
  },
  "permissions": ["storage"],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
